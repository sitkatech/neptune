@using LtInfo.Common.HtmlHelperExtensions
@using Neptune.Web.Views.WaterQualityManagementPlan

@inherits Edit

<div class="row">
    <div class="col-xs-12">
        <div class="form-horizontal">
            @using (Html.BeginForm())
            {
                <div class="form-group">
                    <div class="col-xs-12 col-sm-3 control-label">
                        @Html.LabelWithSugarFor(m => m.WaterQualityManagementPlanName)
                    </div>
                    <div class="col-xs-12 col-sm-9">
                        @Html.TextBoxFor(m => m.WaterQualityManagementPlanName, new {@class = "form-control"})
                        @Html.ValidationMessageFor(m => m.WaterQualityManagementPlanName)
                    </div>
                </div>
                <div class="form-group">
                    <div class="col-xs-12 col-sm-3 control-label">
                        @Html.LabelWithSugarFor(m => m.StormwaterJurisdictionID)
                    </div>
                    <div class="col-xs-12 col-sm-9">
                        @Html.DropDownListFor(m => m.StormwaterJurisdictionID, ViewDataTyped.StormwaterJurisdictionSelectListItems, new {@class = "form-control"})
                        @Html.ValidationMessageFor(m => m.StormwaterJurisdictionID)
                    </div>
                </div>
                <div class="form-group">
                    <div class="col-xs-12 col-sm-3 control-label">
                        @Html.LabelWithSugarFor(m => m.MaintenanceUserPersonID)
                    </div>
                    <div class="col-xs-12 col-sm-9">
                        @Html.EditorFor(m => m.MaintenanceUserPersonID)
                        @Html.ValidationMessageFor(m => m.MaintenanceUserPersonID)
                    </div>
                </div>
                <div class="form-group">
                    <div class="col-xs-12 col-sm-3 control-label">
                        @Html.LabelWithSugarFor(m => m.MaintenanceOrganziationID)
                    </div>
                    <div class="col-xs-12 col-sm-9">
                        @Html.EditorFor(m => m.MaintenanceOrganziationID)
                        @Html.ValidationMessageFor(m => m.MaintenanceOrganziationID)
                    </div>
                </div>
                <div class="form-group">
                    <div class="col-xs-12 col-sm-3 control-label">
                        @Html.LabelWithSugarFor(m => m.WaterQualityManagementPlanPriorityID)
                    </div>
                    <div class="col-xs-12 col-sm-9">
                        @Html.DropDownListFor(m => m.WaterQualityManagementPlanPriorityID, ViewDataTyped.WaterQualityManagementPlanPrioritySelectListItems, new { @class = "form-control" })
                        @Html.ValidationMessageFor(m => m.WaterQualityManagementPlanPriorityID)
                    </div>
                </div>
                <div class="form-group">
                    <div class="col-xs-12 col-sm-3 control-label">
                        @Html.LabelWithSugarFor(m => m.WaterQualityManagementPlanStatusID)
                    </div>
                    <div class="col-xs-12 col-sm-9">
                        @Html.DropDownListFor(m => m.WaterQualityManagementPlanStatusID, ViewDataTyped.WaterQualityManagementPlanStatusSelectListItems, new { @class = "form-control" })
                        @Html.ValidationMessageFor(m => m.WaterQualityManagementPlanStatusID)
                    </div>
                </div>
                <div class="form-group">
                    <div class="col-xs-12 col-sm-3 control-label">
                        @Html.LabelWithSugarFor(m => m.WaterQualityManagementPlanDevelopmentTypeID)
                    </div>
                    <div class="col-xs-12 col-sm-9">
                        @Html.DropDownListFor(m => m.WaterQualityManagementPlanDevelopmentTypeID, ViewDataTyped.WaterQualityManagementPlanDevelopmentTypeSelectListItems, new { @class = "form-control" })
                        @Html.ValidationMessageFor(m => m.WaterQualityManagementPlanDevelopmentTypeID)
                    </div>
                </div>
                <div class="form-group">
                    <div class="col-xs-12 col-sm-3 control-label">
                        @Html.LabelWithSugarFor(m => m.WaterQualityManagementPlanLandUseID)
                    </div>
                    <div class="col-xs-12 col-sm-9">
                        @Html.DropDownListFor(m => m.WaterQualityManagementPlanLandUseID, ViewDataTyped.WaterQualityManagementPlanLandUseSelectListItems, new { @class = "form-control" })
                        @Html.ValidationMessageFor(m => m.WaterQualityManagementPlanLandUseID)
                    </div>
                </div>
            }
        </div>
    </div>
</div>
