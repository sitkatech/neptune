{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "Neptune.Web": {
            "root": "",
            "sourceRoot": "src",
            "projectType": "application",
            "prefix": "",
            "schematics": {
                "@schematics/angular:component": {
                    "style": "scss"
                }
            },
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "outputPath": "wwwroot",
                        "index": "src/index.html",
                        "main": "src/main.ts",
                        "polyfills": "src/polyfills.ts",
                        "tsConfig": "src/tsconfig.app.json",
                        "assets": [
                            "src/assets",
                            {
                                "glob": "**/*",
                                "input": "./node_modules/leaflet/dist/images",
                                "output": "./"
                            },
                            {
                                "glob": "**/*",
                                "input": "node_modules/tinymce",
                                "output": "/tinymce/"
                            }
                        ],
                        "styles": [
                            "src/styles.scss",
                            "node_modules/leaflet.fullscreen/Control.FullScreen.css",
                            "node_modules/leaflet-gesture-handling/dist/leaflet-gesture-handling.css",
                            "node_modules/leaflet-draw/dist/leaflet.draw.css",
                            "node_modules/leaflet-loading/src/Control.Loading.css"
                        ],
                        "scripts": [
                            "node_modules/jquery/dist/jquery.slim.min.js",
                            "node_modules/popper.js/dist/umd/popper.min.js",
                            "node_modules/bootstrap/dist/js/bootstrap.min.js",
                            "node_modules/iso8601-js-period/iso8601.js",
                            "node_modules/tinymce/tinymce.min.js"
                        ],
                        "vendorChunk": true,
                        "extractLicenses": false,
                        "buildOptimizer": false,
                        "sourceMap": true,
                        "optimization": false,
                        "namedChunks": true
                    },
                    "configurations": {
                        "qa": {
                            "budgets": [
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "8kb"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "namedChunks": false,
                            "buildOptimizer": true,
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.qa.ts"
                                }
                            ]
                        },
                        "production": {
                            "budgets": [
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "6kb"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "namedChunks": false,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.prod.ts"
                                }
                            ]
                        }
                    },
                    "defaultConfiguration": ""
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "options": {
                        "host": "neptune.localhost.sitkatech.com",
                        "port": 8213,
                        "buildTarget": "Neptune.Web:build"
                    },
                    "configurations": {
                        "production": {
                            "buildTarget": "Neptune.Web:build:production"
                        }
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "buildTarget": "Neptune.Web:build"
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "src/test.ts",
                        "polyfills": "src/polyfills.ts",
                        "tsConfig": "src/tsconfig.spec.json",
                        "karmaConfig": "src/karma.conf.js",
                        "styles": [
                            "./node_modules/@fortawesome/fontawesome-free/css/all.css",
                            "src/styles.scss"
                        ],
                        "scripts": [],
                        "assets": [
                            "src/favicon.svg",
                            "src/assets"
                        ]
                    }
                },
                "lint": {
                    "builder": "@angular-eslint/builder:lint",
                    "options": {
                        "lintFilePatterns": [
                            "src/**/*.ts",
                            "src/**/*.html"
                        ]
                    }
                }
            }
        },
        "Neptune.Web-e2e": {
            "root": "e2e/",
            "projectType": "application",
            "prefix": "",
            "architect": {
                "e2e": {
                    "builder": "@angular-devkit/build-angular:protractor",
                    "options": {
                        "protractorConfig": "e2e/protractor.conf.js",
                        "devServerTarget": "Neptune.Web:serve"
                    },
                    "configurations": {
                        "production": {
                            "devServerTarget": "Neptune.Web:serve:production"
                        }
                    }
                }
            }
        }
    },
    "cli": {
        "analytics": false
    }
}