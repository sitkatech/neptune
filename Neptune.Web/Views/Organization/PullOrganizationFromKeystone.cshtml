@using Neptune.Web.Common.HtmlHelperExtensions
@inherits Neptune.Web.Views.Organization.PullOrganizationFromKeystone
@model Neptune.Web.Views.Organization.PullOrganizationFromKeystoneViewModel
@using (Html.BeginForm())
{
    <div class="form-horizontal">
        <div class="form-group">
            <div class="col-sm-2 control-label">
                @Html.LabelWithSugarFor(x => x.OrganizationGuid)
            </div>
            <div class="col-sm-10">
                @Html.TextBoxFor(x => x.OrganizationGuid, new { style = "width:100%" })
                @Html.ValidationMessageFor(x => x.OrganizationGuid)
            </div>
        </div>
    </div>


}
